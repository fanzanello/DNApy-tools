DNA = input("Enter your DNA sequence:\n")   # press Enter
DNA_upper = DNA.upper()
DNA_upper_reversed = DNA_upper[::-1]
print()

print("The reversed DNA strand is:\n",DNA_upper_reversed)
print()

print("The reverse complement DNA strand is:")

for i in DNA_upper_reversed:
      if (i == "A"): print("T", end = "")
      elif (i == "T"): print("A", end = "")
      elif (i == "C"): print("G", end = "")
      elif (i == "G"): print("C", end = "")
